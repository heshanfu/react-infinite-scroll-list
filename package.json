{
  "name": "react-infinite-scroll-list",
  "version": "1.2.0",
  "description": "React component for create an inifinte list",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist lib",
    "start": "webpack-dev-server --config webpack.example.config.js --inline --hot --content-base dist --no-info --open",
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint src example",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore **/*.spec.js",
    "build:umd": "webpack src/index.js dist/react-infinite-list.js --config webpack.config.js --progress",
    "build:umd:min": "webpack src/index.js dist/react-infinite-list.min.js --config webpack.config.js --env.production --progress ",
    "test": "npm run mocha -- \"src/**/*.spec.js\"",
    "mocha": "mocha --require babel-register --require test-setup.js",
    "mocha:watch": "npm run mocha -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-infinite-list.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "infinite",
    "scroll",
    "list",
    "react"
  ],
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  }
}
